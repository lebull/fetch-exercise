{
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier",
      "react"
    ],
    "rules": {
      "react/prop-types": 0,
      "import/no-unresolved": [2, { "caseSensitive": false }],
      "semi": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": [
        "error", {
          "endOfLine": "auto"
        }
      ]
    }
  }